<table cellpadding="100" border= "1px solid black"; bgcolor="#f0f8ff">
    <tr>
        <th> Name</th>
        <th> age</th>
    </tr>
  <% @users.each do |user| %>
    <tr>
        <td><%= user.username %></td>
        <td><%= user.age %></td>
    </tr>
  <% end %>
</table>
<%= will_paginate @users, :remote => true %>